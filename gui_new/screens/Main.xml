        <GuiElement type="button" template="cpMainCategorie_CourseManager"  onEnter="onEnableHelp" onLeave="onDisableHelp" onClick="onOpenCourseManager" parameter="courseManager" />
		    <!--Speed Settings-->
        <GuiElement type="button" template="cpMainCategorie_Speed" onEnter="onEnableHelp" onLeave="onDisableHelp" onClick="onClickOpenPage" parameter="speed" id="guiBtnSpeed" /> 
        <!--Unloader Control [Page0]-->
        <GuiElement type="button" template="cpMainCategorie_Drivers" onEnter="onEnableHelp" onLeave="onDisableHelp" onClick="onClickOpenPage" parameter="drivers" id="guiBtnDrivers" />
		    
        
        
        <!--Combine Manager-->
        <GuiElement type="button" template="cpMainCategorie_DriversSearch" onEnter="onEnableHelp" onLeave="onDisableHelp" onClick="onClickOpenPage" parameter="driversSearch" id="guiBtnDriversSearch" />
