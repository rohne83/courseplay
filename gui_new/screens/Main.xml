      </GuiElement>

	  <!--pageLoad for Buttons-->
      <GuiElement type="flowLayout" template="cpMainLayout2">
        <GuiElement type="element" template="cpMainPagePosition">
          <GuiElement type="pageLoad" gui="CpGuiPageSteering" pageId="steering" buttonId="guiBtnSteering" />
          <GuiElement type="pageLoad" gui="CpGuiPageDrivers" pageId="drivers" buttonId="guiBtnDrivers" />
          <GuiElement type="pageLoad" gui="CpGuiPageDriversSearch" pageId="driversSearch" buttonId="guiBtnDriversSearch" />
          <GuiElement type="pageLoad" gui="CpGuiPageDriversSpeed" pageId="speed" buttonId="guiBtnSpeed" />
        </GuiElement>
        
