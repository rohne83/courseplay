<GUI>  
  <GuiElement type="image" template="cpMain" id="gui_dialog">  
	<!--Header-->
    <GuiElement type="image" template="cpMainBarTop">  
        <GuiElement type="guiMover" template="cpMainGuiMover" target="gui_dialog" /> 
        <GuiElement type="image" template="cpMainLogo" /> 
        <GuiElement type="text" template="cpMainTitle1" text="COURSE" /> 
        <GuiElement type="text" template="cpMainTitle2" text="PLAY" /> 
        <GuiElement type="text" template="cpMainVersion" text="v6.04.00001.dev" /> 
